/* pages/customer/customerIndex/customerIndex.wxss */
.contain{
  height: 100%;
  width: 100%;
}
.custom-index{
    height: 87%;
    font-size: 14px;
}
view>text{
    display: inline-block;
    float: left;
    line-height: 32px;
}
.item-address{
    clear: both;
    line-height: 32px;
    width: 100%;
    color: rgb(56, 67, 74);
}
.item-title{
    width: 70%;
}
.item-level{
    width: 30%;
    text-align: right;
    color: rgb(231, 76, 60);
}
.item-contact>text{
    width: 50%;
    color: rgb(56, 67, 74);
}
.search-group{
    padding: 0 0 2px 0;
}
.item-group{
    margin-top: 2px;
}
.blockface {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
}
